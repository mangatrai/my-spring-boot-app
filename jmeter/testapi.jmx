          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">
              import java.util.UUID
              import java.text.SimpleDateFormat

              def EVENT_STATES = [&quot;VALIDATED&quot;, &quot;CREATED&quot;, &quot;RESCHEDULED&quot;, &quot;CANCELLED&quot;, &quot;MODIFIED&quot;]
              def RANDOM = new Random()

              def prefix = vars.get(&quot;prefix&quot;)
              def id = UUID.randomUUID().toString()

                  // Increment the global counter
    		    def counter = props.get(&quot;eventIdentifierCounter&quot;).toInteger() + 1
    		    props.put(&quot;eventIdentifierCounter&quot;, counter.toString())
    		    
    		    def eventIdentifier = prefix + &quot;_MXND_&quot; + counter
              def eventTimestamp = new Date().time
              def eventState = EVENT_STATES[RANDOM.nextInt(EVENT_STATES.size())]
              def eventName = &quot;com.sephora.happpening.reservation.&quot; + eventState.toLowerCase()
              def datePattern = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm&apos;Z&apos;&quot;).format(new Date())
              def eventPayloadUrl = &quot;https://sepeus1lowerhasm01.blob.core.windows.net/auditing-cloud-event/&quot; + eventName + &quot;_&quot; + datePattern

              vars.put(&quot;id&quot;, id)
              vars.put(&quot;eventIdentifier&quot;, eventIdentifier)
              vars.put(&quot;eventTimestamp&quot;, eventTimestamp.toString())
              vars.put(&quot;eventState&quot;, eventState)
              vars.put(&quot;eventName&quot;, eventName)
              vars.put(&quot;eventPayloadUrl&quot;, eventPayloadUrl)

              // Write eventIdentifier to a CSV file
              def eventIdentifierFileName = vars.get(&quot;eventIdentifierFileName&quot;)
            def file = new File(eventIdentifierFileName)
            file.append(&quot;${eventIdentifier},${eventState},${eventName}\n&quot;)
            </stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
          </JSR223PreProcessor>
